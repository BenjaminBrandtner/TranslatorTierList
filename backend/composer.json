{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "^7.4",
    "fideloper/proxy": "^4.2",
    "flynsarmy/csv-seeder": "2.*",
    "fruitcake/laravel-cors": "^2.0",
    "google/apiclient": "^2.7",
    "google/apiclient-services": "v0.156",
    "guzzlehttp/guzzle": "^7.0.1",
    "laravel/framework": "^8.0",
    "laravel/nova": "*",
    "laravel/tinker": "^2.0",
    "orangehill/iseed": "^3.0",
    "sentry/sentry-laravel": "^2.0",
    "spatie/laravel-backup": "^6.11"
  },
  "require-dev": {
    "barryvdh/laravel-ide-helper": "^2.8",
    "facade/ignition": "^2.3.6",
    "fzaninotto/faker": "^1.9.1",
    "laravel/dusk": "^6.6",
    "mockery/mockery": "^1.3.1",
    "nunomaduro/collision": "^5.0",
    "phpunit/phpunit": "^9.0"
  },
  "repositories": [
    {
      "type": "path",
      "url": "./nova_src"
    }
  ],
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "process-timeout": 0
  },
  "extra": {
    "laravel": {
      "dont-discover": [
        "laravel/nova"
      ]
    },
    "google/apiclient-services": [
      "YouTube"
    ]
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "helpIde": [
      "php artisan clear-compiled",
      "php artisan ide-helper:generate",
      "php artisan ide-helper:meta",
      "php artisan ide-helper:models -W -R"
    ],
    "test": "php artisan test --exclude-group ThirdPartyApi",
    "testYoutube": "php artisan test --group ThirdPartyApi",
    "testWatch": "phpunit-watcher watch --exclude-group ThirdPartyApi --printer NunoMaduro\\Collision\\Adapters\\Phpunit\\Printer",
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-update-cmd": "Google_Task_Composer::cleanup",
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  }
}
